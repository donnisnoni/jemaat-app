<head>
  <meta content="text/html; charset=UTF-8" http-equiv="content-type" />
  <style type="text/css">
    :root {
      -webkit-print-color-adjust: exact;
    }

    body {
      margin: 20px;
      padding: 0;
    }

    * {
      padding: 0;
      margin: 0;
    }

    table {
      border-collapse: collapse;
      border: none;
      height: 100%;
    }

    body,
    pre.normal {
      font-family: sans-serif;
    }

    thead,
    tbody tr {
      border-bottom: 1px solid grey;
    }

    thead {
      border-top: 1px solid grey;
    }

    thead td {
      background-color: lightgray;
      padding: 10px;
      font-weight: bold;
    }

    tr:last-child td {
      border-bottom: none;
    }

    tbody td,
    tfoot td {
      padding: 5px;
    }

    .text-center {
      text-align: center;
    }

    .text-right {
      text-align: right;
    }

    .font-bold {
      font-weight: bold;
    }

    @page {
      size: auto;
      marks: none;
    }
  </style>
</head>
