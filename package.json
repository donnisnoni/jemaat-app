{
  "name": "jemaat-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "compile": "cross-env NODE_ENV=production vite build",
    "check": "svelte-check --human && tsc --noEmit",
    "watch:svelte": "svelte-check --human --watch",
    "watch:ts": "tsc --noEmit --watch",
    "start": "run-p watch:* dev",
    "build": "run-s check compile",
    "serve": "sirv dist"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@svitejs/vite-plugin-svelte": "^0.11.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.14.37",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "moment": "^2.29.1",
    "postcss": "^8.2.8",
    "postcss-import": "^14.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "sass": "^1.32.8",
    "sequelize-cli": "^6.2.0",
    "sirv-cli": "^1.0.11",
    "svelte": "^3.35.0",
    "svelte-check": "^1.3.0",
    "svelte-preprocess": "^4.7.0",
    "svelte-spa-router": "^3.1.0",
    "tailwindcss": "^2.0.4",
    "typescript": "^4.2.3",
    "vite": "^2.1.3",
    "vite-aliases": "^0.5.1"
  },
  "dependencies": {
    "ajv-errors": "^2.0.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "deep-equal": "^2.0.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "fastify": "^3.14.1",
    "fastify-jwt": "^2.4.0",
    "mariadb": "^2.5.3",
    "pdf-lib": "^1.16.0",
    "puppeteer": "^8.0.0",
    "query-string-lite": "^0.2.2",
    "sequelize": "^6.6.2"
  }
}
