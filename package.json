{
  "name": "jemaat-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "compile": "cross-env NODE_ENV=production vite build",
    "check": "svelte-check --human && tsc --noEmit",
    "watch:svelte": "svelte-check --human --watch",
    "watch:ts": "tsc --noEmit --watch",
    "start": "run-p watch:* dev",
    "build": "run-s check compile",
    "serve": "sirv dist",
    "postinstall": "rm -rf ./node_modules/@vue/compiler-sfc/node_modules/postcss/ &&  cp -r ./node_modules/postcss/ ./node_modules/@vue/compiler-sfc/node_modules/"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.14.28",
    "autoprefixer": "^10.2.4",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "moment": "^2.29.1",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.6",
    "sass": "^1.32.7",
    "sequelize-cli": "^6.2.0",
    "sirv-cli": "^1.0.11",
    "svelte": "^3.32.3",
    "svelte-check": "^1.1.35",
    "svelte-preprocess": "^4.6.9",
    "svelte-spa-router": "^3.1.0",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.1.5",
    "vite": "^1.0.0-rc.13",
    "vite-aliases": "^0.1.3",
    "vite-plugin-svelte": "^3.0.1"
  },
  "dependencies": {
    "ajv-errors": "^2.0.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "deep-equal": "^2.0.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "fastify": "^3.12.0",
    "fastify-jwt": "^2.3.0",
    "mariadb": "^2.5.3",
    "pdf-lib": "^1.16.0",
    "puppeteer": "^7.1.0",
    "query-string-lite": "^0.2.2",
    "sequelize": "^6.5.0"
  }
}
